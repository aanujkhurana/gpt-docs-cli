{
  "name": "gpt-docs",
  "version": "1.0.0",
  "description": "CLI tool that uses OpenAI to automatically generate JSDoc-style comments for JavaScript/TypeScript code",
  "main": "dist/index.js",
  "bin": {
    "gpt-docs": "./dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/bin/cli.js",
    "dev": "ts-node src/bin/cli.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "openai",
    "gpt",
    "documentation",
    "jsdoc",
    "typescript",
    "javascript",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^10.0.1",
    "dotenv": "^16.0.3",
    "glob": "^10.0.0",
    "keytar": "^7.9.0",
    "openai": "^3.2.1",
    "ora": "^5.4.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}